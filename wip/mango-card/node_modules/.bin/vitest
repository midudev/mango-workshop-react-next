#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/vitest@2.1.5_@types+node@22.9.1_happy-dom@15.11.6_msw@2.6.5_@types+node@22.9.1_/node_modules/vitest/node_modules:/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/vitest@2.1.5_@types+node@22.9.1_happy-dom@15.11.6_msw@2.6.5_@types+node@22.9.1_/node_modules:/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/vitest@2.1.5_@types+node@22.9.1_happy-dom@15.11.6_msw@2.6.5_@types+node@22.9.1_/node_modules/vitest/node_modules:/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/vitest@2.1.5_@types+node@22.9.1_happy-dom@15.11.6_msw@2.6.5_@types+node@22.9.1_/node_modules:/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
