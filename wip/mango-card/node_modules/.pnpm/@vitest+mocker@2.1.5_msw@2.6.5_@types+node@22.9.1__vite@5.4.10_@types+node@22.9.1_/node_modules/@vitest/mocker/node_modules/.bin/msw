#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/msw@2.6.5_@types+node@22.9.1/node_modules/msw/cli/node_modules:/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/msw@2.6.5_@types+node@22.9.1/node_modules/msw/node_modules:/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/msw@2.6.5_@types+node@22.9.1/node_modules:/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/msw@2.6.5_@types+node@22.9.1/node_modules/msw/cli/node_modules:/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/msw@2.6.5_@types+node@22.9.1/node_modules/msw/node_modules:/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/msw@2.6.5_@types+node@22.9.1/node_modules:/Users/midudev/Dev/mango-workshop-react-next/wip/mango-card/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../msw@2.6.5_@types+node@22.9.1/node_modules/msw/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../../msw@2.6.5_@types+node@22.9.1/node_modules/msw/cli/index.js" "$@"
fi
